#!/usr/bin/env python3
import sys
from IPython import embed


if __name__ == "__main__":
    uipy3_filename = ".default.uipy3"
    
    if len(sys.argv) == 2 :
        uipy3_filename = sys.argv[1]
    
    exec(open(uipy3_filename).read())
    
    print ("uipy -> " + uipy3_filename)
    
    embed()
